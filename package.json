{
  "name": "proxy-string-parser",
  "version": "1.0.4",
  "description": "Parsing proxy string",
  "keywords": [
    "proxy",
    "string",
    "parser",
    "format"
  ],
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislav-puzyrev/proxy-string-parser.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "yarn run docs:typedoc && yarn run docs:concat",
    "docs:typedoc": "typedoc --plugin typedoc-plugin-markdown --name API --hideBreadcrumbs src/index.ts",
    "docs:concat": "concat-md --hide-anchor-links docs > README.md",
    "test": "yarn run test:typescript && yarn run test:eslint && yarn run test:ncu",
    "test:typescript": "tsc --noEmit",
    "test:eslint": "eslint .",
    "test:ncu": "ncu --errorLevel 2"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concat-md": "^0.5.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "npm-check-updates": "^16.4.0",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  }
}

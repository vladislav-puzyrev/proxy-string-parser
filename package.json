{
  "name": "proxy-string-parser",
  "version": "3.0.0",
  "description": "Parsing proxy string",
  "keywords": [
    "proxy",
    "string",
    "parser",
    "format"
  ],
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislav-puzyrev/proxy-string-parser.git"
  },
  "scripts": {
    "build": "yarn run clean:build && tsc --project tsconfig.production.json",
    "docs": "yarn run clean:docs && yarn run docs:typedoc && yarn run docs:concat",
    "docs:typedoc": "typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs --name API src/index.ts",
    "docs:concat": "concat-md --hide-anchor-links docs > README.md",
    "clean": "yarn run clean:build && yarn run clean:docs",
    "clean:build": "rimraf build",
    "clean:docs": "rimraf docs",
    "test": "yarn run test:typescript && yarn run test:eslint && yarn run test:jest",
    "test:typescript": "tsc --noEmit",
    "test:eslint": "eslint .",
    "test:jest": "jest ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concat-md": "^0.5.0",
    "eslint": "^8.29.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  }
}
